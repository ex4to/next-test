{
  "extends": [
    "next/core-web-vitals",
    "airbnb",
    "plugin:prettier/recommended"
  ],
  "parserOptions": {
    "ecmaFeatures": {
        "jsx": true
    },
    "ecmaVersion": "latest",
    "sourceType": "module"
},
"plugins": [
    "react"
],
"rules": {
    "react/jsx-filename-extension": [1, {"extensions": [".js", ".jsx"]}],
    "react/function-component-definition": [2, { "namedComponents": "arrow-function" }]
}
}
